<div class="qna_list_wrap w1760 mt100">
  <h3>Q&A</h3>
  <article class="qna_list_table_wrap">
  <div class="qna_list_title">
    <ul>
      <li class="qna_list_num">번호</li>
      <li class="qna_list_cont">제목</li>
      <li class="qna_list_write">작성자</li>
      <li class="qna_list_day">등록일</li>
      <li class="qna_list_view">조회수</li>
    </ul>
  </div>
  <% rows.forEach((row,index)=>{  %>
  <div class="qna_list_body">
    <ul class="qna_list">
      <li class="qna_list_num"><%= row.num %></li>
      <li class="qna_list_cont"><%= row.title %></li>
      <li class="qna_list_write"><%= row.wirte %></li>
      <li class="qna_list_day"><%= row.create_time %></li>
      <li class="qna_list_view"><%= row.view %></li>
    </ul>
    <div class="qna_list_content">
        <p><%= row.cont %> </p>
      <!-- 수정,삭제버튼 -->
        <div class="emend_delete_qna_btn_box mt20">
          <button type="button" id="qna_emend" onclick="javascript:location.href='/updateQ?num=<%= row.num %>'">수정</button>
          <button type="button" id="qna_delete" onclick="javascript:location.href='/deleteQ?num=<%= row.num %>'">삭제</button>
        </div>
      </div>
    </div>
    <% }); %>
    <!-- 글작성 버튼 -->
    <div class="write_qna_btn_box mt40">
      <button type="button" id="qna_write" onclick="location.href='/qna_new'">글쓰기</button>
    </div>
  </article>
</div>
<script src="js/qna.js"></script>